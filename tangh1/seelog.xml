<seelog type="asynctimer" asyncinterval="1000000" minlevel="debug" maxlevel="error">
    <outputs formatid="main">
        <console/>
        <file path="log.log"/>
    </outputs>
    <formats>
        <format id="main" format="%UTCDate %UTCTime [%LEV] %RelFile %Line %Msg%n"/>
    </formats>
</seelog>